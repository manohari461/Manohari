def reverseString(s):
    
    left, right = 0, len(s) - 1
    while left < right:
        s[left], s[right] = s[right], s[left]
        left += 1
        right -= 1

# Example usage
s = ["h", "e", "l", "l", "o"]
print("Original:", s)
reverseString(s)
print("Reversed:", s)

s = ["H", "a", "n", "n", "a", "h"]
print("\nOriginal:", s)
reverseString(s)
print("Reversed:", s)
